{
	"click": [
		{
			"selector": "a.article_link",
			"function": "$this.open_article",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_main_window #cinemads_pages #cinemads_page_next",
			"function": "$this.load_next_page",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_main_window #cinemads_pages #cinemads_page_prev",
			"function": "$this.load_prev_page",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_main_window .cinema_ds_video_entry:not(.cinema_ds_video_entry[autoplay])",
			"function": "$this.maximize_video",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_main_window .cinema_ds_video_entry[autoplay]",
			"function": "$this.maximize_video_autoplay",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_main_window .cinema_ds_img_entry",
			"function": "$this.maximize_image",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "#cinema_ds_fullscreen:not(.noclick)",
			"function": "$('body #cinema_ds_fullscreen').remove",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": ".cinemads_page:not(.pg_active)",
			"function": "$this.page_switch",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		}
	],
	"keydown": [
		{
			"selector": "body",
			"function": "$this.close_video_fullscreen",
			"pass_event": true,
			"pass_element": false,
			"pass_params": ""
		}
	]
}